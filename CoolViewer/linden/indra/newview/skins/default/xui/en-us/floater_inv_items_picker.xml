<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater name="floater_inv_items_picker" title="Inventory picker"
 can_close="true" can_minimize="true" can_resize="true" can_drag_on_left="false"
 width="280" height="344" min_width="280" min_height="344">
	<text name="select_text" font="SansSerif"
	 left="10" bottom="-36" width="220" height="16" follows="left|top">
		Select an item:
	</text>
	<search_editor name="search_editor" label="Type here to search"
	 left="10" bottom_delta="-20" width="260" height="18" follows="left|top|right" />
	<inventory_panel name="inventory_panel" allow_multi_select="false" disable_double_click="true"
	 sort_order="InventoryPickerSortOrder"
	 left="10" bottom_delta="-255" width="260" height="250" follows="left|top|right|bottom" />
	<button name="select_btn" label="Select" font="SansSerif"
	 left="10" bottom="6" width="80" height="20" follows="left|bottom" />
	<button name="close_btn" label="Cancel" font="SansSerif"
	 right="-10" bottom_delta="0" width="80" height="20" follows="right|bottom" />
	<string name="title">Inventory ([ASSETTYPE]) picker</string>
</floater>
